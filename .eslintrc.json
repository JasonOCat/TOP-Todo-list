{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": "off",
    "no-use-before-define": "off",
    "import/no-cycle": "off",
    "implicit-arrow-linebreak": "off",
    "no-param-reassign": "warn",
    "operator-linebreak": "off",
    "comma-dangle": "off"
  }
}
